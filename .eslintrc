{
  "root": true,
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
  },
  "rules": {
      "prettier/prettier": ["error"],
      "react-hooks/exhaustive-deps": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}